{
  "type": "symfony-bundle",
  "name": "jav/apitopia-bundle",
  "description": "APIs made simpler",
  "license": "MIT",
  "keywords": [
    "symfony",
    "api",
    "rest",
    "graphql"
  ],
  "version": "1.0",
  "authors": [
    {
      "name": "Jérémy Vienne",
      "email": "jeremyvienne@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ecodev/graphql-upload": "^7.0",
    "ivome/graphql-relay-php": "dev-master",
    "nyholm/psr7": ">=1.6",
    "symfony/routing": ">=5.2",
    "symfony/dependency-injection": ">=5.2",
    "symfony/http-kernel": ">=5.2",
    "symfony/config": ">=5.2",
    "symfony/serializer": ">=5.2",
    "symfony/property-access": ">=5.2",
    "symfony/psr-http-message-bridge": "^2.2",
    "symfony/console": ">=5.2",
    "symfony/finder": ">=5.2",
    "symfony/http-client": ">=5.2",
    "symfony/mime": ">=5.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "jetbrains/phpstorm-attributes": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Jav\\ApiTopiaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jav\\ApiTopiaBundle\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {"type":  "vcs", "url":  "git@github.com:otzy/graphql-relay-php.git"}
  ]
}
